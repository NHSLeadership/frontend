{
  "name": "nhsuk-frontend",
  "version": "0.1.0",
  "description": "A collection of front-end components for NHS sites",
  "main": "dist/lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhsuk/frontend.git"
  },
  "author": "NHS.UK developers",
  "license": "MIT",
  "dependencies": {
    "@frctl/fractal": "^1.1.3",
    "@frctl/mandelbrot": "^1.1.0",
    "@frctl/nunjucks": "^1.0.3",
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-flatten": "^0.3.1",
    "gulp-inject": "^4.2.0",
    "gulp-postcss": "^6.3.0",
    "gulp-sass": "^3.1.0",
    "jquery": "1.12",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "node-sass-import-once": "^1.2.0",
    "nunjucks": "^3.0.0",
    "run-sequence": "^1.2.2",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "gulp-watch": "^4.3.11"
  },
  "scripts": {
    "build": "gulp build",
    "dev": "fractal start --sync --port 4000",
    "build:docs": "NODE_ENV=production fractal build && node ./fractal/create-redirect.js",
    "deploy": "npm run build:docs && gh-pages --add -d 'build'",
    "js:lib": "babel lib -d $npm_package_main -s",
    "js:client": "webpack -p",
    "postinstall": "gulp build:dist"
  }
}
